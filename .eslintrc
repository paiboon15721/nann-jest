{
  "extends": "react-app",
  "rules": {
    "semi": ["warn", "never"],
    "import/no-unresolved": "error",
    "comma-dangle": ["error", "always-multiline"],
    "jsx-a11y/anchor-is-valid": 0
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
  }
}
